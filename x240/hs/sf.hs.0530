lucky :: (Integral a) => a -> String
lucky 7 = "luck number seven"
lucky x = "sorry not seven"

sayMe :: (Integral a) => a -> String
sayMe 1 = "one"
sayMe 2 = "two"
sayMe 3 = "three"
sayMe 4 = "four"
sayMe 5 = "five"
sayMe x = "out of 1-5"

factorial :: (Integral a)  => a -> a
factorial 0 = 1
factorial n = n * factorial(n-1)
